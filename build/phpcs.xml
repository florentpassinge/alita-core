<?xml version="1.0" encoding="utf-8" ?>
<ruleset name="alita Standard">
    <description>An sf2 custom coding standard</description>
    <rule ref="./tools/phpcs/Symfony2"/>
    <rule ref="Generic.Files.LineLength">
        <properties>
            <property name="lineLimit" value="120"/>
            <property name="absoluteLineLimit" value="150"/>
        </properties>
    </rule>
</ruleset>